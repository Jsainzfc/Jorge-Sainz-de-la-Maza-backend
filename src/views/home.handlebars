<h1>Lista de productos:</h1>
<ul class="products-list">
    {{#each products}}
    <li>
        <p><b>Título:</b> {{this.title}}</p>
        <p><b>Descripción:</b> {{this.description}}</p>
        <p><b>Precio:</b> {{this.price}}€</p>
        <p><b>Stock:</b> {{this.stock}}</p>
    </li>
    {{/each}}
</ul>
